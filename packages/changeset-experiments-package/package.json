  },
  "devDependencies": {
    "@ducktors/tsconfig": "^0.1.1",
    "@types/node": "^16.18.11",
    "@vitest/coverage-istanbul": "^0.28.4",
    "rome": "11.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4"
  },
  "files": [
    "dist"
  ]
}