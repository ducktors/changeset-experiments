{
  "name": "@ducktors/changeset-experiments-package",
  "version": "0.2.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "pnpm build",
    "build": "tsc --noEmit && tsup-node",
    "format": "rome format --write .",
